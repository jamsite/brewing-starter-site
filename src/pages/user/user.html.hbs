---
[route]
repeat = "{{users}}"
path = "{{this.id}}.html"
rewrite = "{{this.id}}/details"

[data]
user = "{{this}}"
id = "{{this.id}}"
name = "{{this.name}}"
---
{{#> layout-default}}
<p>Hello from user page

<pre>
id: {{id}}
name: {{name}}
user.id: {{user.id}}
user.name: {{user.name}}
</pre>
{{/layout-default}}
