    "micro-helmet": "^1.0.1"
  },
  "devDependencies": {
    "@jamsite/jamsite-dev": "0.0.24",
    "rollup": "^1.29.0",
    "rollup-plugin-output-manifest": "^1.0.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-manifest-plugin": "^2.2.0"
